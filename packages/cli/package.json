{
  "name": "lazykitty",
  "version": "0.0.1",
  "description": "CLI for LazyKitty - build infrastructure for Expo apps",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "lazykitty": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@lazykitty/shared": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "form-data": "^4.0.1",
    "ignore": "^6.0.2",
    "ora": "^8.1.1",
    "qrcode-terminal": "^0.12.0",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/qrcode-terminal": "^0.12.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
